{
  "version": "1.1.3-*",
  "description": "A c# library to add jitter to cache durations and retry intervals to increase entropy in your system and prevent thundering herds",

  "packOptions": {
    "projectUrl": "https://github.com/JorritSalverda/JitterMagic",
    "licenseUrl": "https://raw.githubusercontent.com/JorritSalverda/JitterMagic/master/LICENSE",
    "repository": {
      "type": "git",
      "url": "git://github.com/JorritSalverda/JitterMagic.git"
    }
  },

  "authors": [ "Jorrit Salverda" ],
  "tooling": {
    "defaultNamespace": "JitterMagic"
  },
  
  "buildOptions": {
     "warningsAsErrors": true
  },

  "frameworks": {
    "netstandard1.0": {
      "dependencies": {
        "System.Runtime": "4.3.0",
        "System.Runtime.Extensions": "4.3.0"
      }
    },
    "net20": {},
    "net35": {},
    "net40": {},
    "net45": {}
  }
}
